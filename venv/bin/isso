#!/home/bnjoroge/PythonProjects/Flaskium/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'isso==0.12.2','console_scripts','isso'
__requires__ = 'isso==0.12.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('isso==0.12.2', 'console_scripts', 'isso')()
    )
